<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<AnchorPane xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml" styleClass="navbar_container"
            fx:controller="com.example.javafx.controller.NavbarController" stylesheets="@../css/navbar.css"
            prefWidth="1700.0" prefHeight="40">
    <children>
        <!-- Navbar -->
        <HBox styleClass="navbar" prefWidth="1700">
            <VBox alignment="CENTER_LEFT" prefWidth="300">
                <ImageView fx:id="logo" pickOnBounds="true" preserveRatio="true"/>
            </VBox>
            <VBox alignment="CENTER_RIGHT" prefWidth="1200" layoutX="300">
                <Button styleClass="logoutButton" fx:id="logoutButton"
                        onMouseClicked="#logout">Logout
                </Button>
            </VBox>
        </HBox>
    </children>
</AnchorPane>