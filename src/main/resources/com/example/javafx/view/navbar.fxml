<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<AnchorPane xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml" styleClass="navbar_container"
            fx:controller="com.example.javafx.controller.NavbarController" stylesheets="@../css/navbar.css"
            prefWidth="1080.0">
    <children>
        <!-- Navbar -->
        <MenuBar styleClass="navbar">
            <menus>
                <Menu mnemonicParsing="false">
                    <graphic>
                        <ImageView fx:id="logo" pickOnBounds="true"
                                   preserveRatio="true"/>
                    </graphic>
                </Menu>
            </menus>
        </MenuBar>
    </children>
</AnchorPane>