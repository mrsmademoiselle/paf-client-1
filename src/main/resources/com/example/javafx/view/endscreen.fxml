<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import javafx.scene.text.Text?>
<Pane xmlns="http://javafx.com/javafx" fx:id="page"
      xmlns:fx="http://javafx.com/fxml"
      stylesheets="@../css/endscreen.css"
      fx:controller="com.example.javafx.controller.EndScreenController">
    <fx:include fx:id="navbar" source="navbar.fxml"/>

    <Pane prefHeight="${controller.heightWithOffset}" prefWidth="${controller.widthWithOffset}"
          layoutY="${controller.offsetY}" layoutX="${controller.offsetX}">
        <Text fx:id="finalText">Hello World</Text>
    </Pane>

</Pane>